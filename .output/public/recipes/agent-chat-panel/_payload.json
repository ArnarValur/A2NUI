":83,"path":84,"stem":85},"TextField","/components/textfield","2.components/18.textfield",{"title":87,"path":88,"stem":89},"CheckBox","/components/checkbox","2.components/19.checkbox",{"title":91,"path":92,"stem":93},"Slideover","/components/slideover","2.components/20.slideover",{"title":95,"path":96,"stem":97},"Table","/components/table","2.components/21.table",{"title":99,"path":100,"stem":101,"children":102,"icon":104},"Recipes","/recipes","3.recipes/01.index",[103,105,109,113,117],{"title":99,"path":100,"stem":101,"icon":104},"i-lucide-chef-hat",{"title":106,"path":107,"stem":108},"Booking Receipt","/recipes/booking-receipt","3.recipes/02.booking-receipt",{"title":110,"path":111,"stem":112},"Establishment Detail","/recipes/establishment-detail","3.recipes/03.establishment-detail",{"title":114,"path":115,"stem":116},"Booking History","/recipes/booking-history","3.recipes/04.booking-history",{"title":118,"path":119,"stem":120},"Agent Chat Panel","/recipes/agent-chat-panel","3.recipes/05.agent-chat-panel",{"id":122,"title":118,"body":123,"description":649,"extension":650,"links":651,"meta":652,"navigation":653,"path":119,"seo":654,"stem":120,"__hash__":655},"docs/3.recipes/05.agent-chat-panel.md",{"type":124,"value":125,"toc":639},"minimark",[126,130,144,147,152,162,173,177,185,190,250,254,260,264,271,277,420,426,433,444,530,534,537,622,635],[127,128,118],"h1",{"id":129},"agent-chat-panel",[131,132,133,134,138,139,143],"p",{},"A prototype demonstrating how an ",[135,136,137],"strong",{},"AI agent"," renders interactive A2UI surfaces directly inside a chat conversation, hosted within Nuxt UI's ",[140,141,142],"code",{},"DashboardPanel",".",[131,145,146],{},"This is the architectural pattern for embedding AI-generated UIs in your admin and business portals.",[148,149,151],"h2",{"id":150},"the-architecture","The Architecture",[153,154,159],"pre",{"className":155,"code":157,"language":158},[156],"language-text","DashboardPanel\n├── #header  →  Agent branding + status indicator\n├── #body    →  Chat messages (scrollable)\n│   ├── User bubble (plain text)\n│   └── Assistant message\n│       ├── Text response\n│       └── A2uiRenderer (inline surface)\n│           └── Table / Cards / any A2UI composition\n└── #footer  →  Chat prompt input\n","text",[140,160,157],{"__ignoreMap":161},"",[131,163,164,165,168,169,172],{},"Each assistant message can contain ",[135,166,167],{},"both text and A2UI JSONL",". The renderer processes the JSONL through ",[140,170,171],{},"useA2uiSurface()"," and renders the resulting surface directly inside the chat flow. The user sees rich, interactive components—tables, stat cards, slideovers—inline with the conversation.",[148,174,176],{"id":175},"live-demo","Live Demo",[131,178,179,180,184],{},"Visit ",[181,182,183],"a",{"href":183},"/agent"," to interact with the prototype.",[131,186,187],{},[135,188,189],{},"Try these commands (in Norwegian):",[191,192,193,206],"table",{},[194,195,196],"thead",{},[197,198,199,203],"tr",{},[200,201,202],"th",{},"Command",[200,204,205],{},"What the Agent Renders",[207,208,209,220,230,240],"tbody",{},[197,210,211,217],{},[212,213,214],"td",{},[140,215,216],{},"Vis dagens bestillinger",[212,218,219],{},"Booking table with 5 columns × 5 rows",[197,221,222,227],{},[212,223,224],{},[140,225,226],{},"Vis ukens omsetning",[212,228,229],{},"3 stat cards + revenue breakdown table",[197,231,232,237],{},[212,233,234],{},[140,235,236],{},"Vis ansatte i dag",[212,238,239],{},"Staff roster table",[197,241,242,247],{},[212,243,244],{},[140,245,246],{},"Hjelp",[212,248,249],{},"4-card grid of capabilities",[148,251,253],{"id":252},"data-flow","Data Flow",[153,255,258],{"className":256,"code":257,"language":158},[156],"1. User types \"Vis dagens bestillinger\"\n2. Message added to chat (user role)\n3. Agent processes intent [mock: keyword match / prod: WebSocket → Agent]\n4. Agent streams response:\n   ├── Text: \"Her er dagens bestillinger:\"\n   ├── A2UI: {\"createSurface\": ...}\n   └── A2UI: {\"updateComponents\": ..., components: [Table, ...]}\n5. onA2uiLine callback feeds each line in real-time\n6. useA2uiSurface().feed() processes JSONL → Surface created\n7. A2uiRenderer renders Surface inline within chat message\n8. User sees interactive table inside the conversation\n",[140,259,257],{"__ignoreMap":161},[148,261,263],{"id":262},"key-components","Key Components",[265,266,268],"h3",{"id":267},"agentchatpanelvue",[140,269,270],{},"AgentChatPanel.vue",[131,272,273,274,143],{},"The main component. Lives at ",[140,275,276],{},"app/components/AgentChatPanel.vue",[153,278,282],{"className":279,"code":280,"language":281,"meta":161,"style":161},"language-vue shiki shiki-themes material-theme-lighter material-theme material-theme-palenight","\u003CUDashboardPanel :id=\"panelId\" :resizable=\"resizable\">\n  \u003Ctemplate #header> Agent branding + status \u003C/template>\n  \u003Ctemplate #body>\n    \u003C!-- Messages loop -->\n    \u003Cdiv v-for=\"msg in messages\">\n      \u003C!-- User bubbles -->\n      \u003C!-- Assistant: text + inline A2uiRenderer -->\n      \u003CA2uiRenderer\n        v-if=\"surfaces.get(surfaceId)\"\n        :surface=\"surfaces.get(surfaceId)!\"\n      />\n    \u003C/div>\n  \u003C/template>\n  \u003Ctemplate #footer> Chat prompt \u003C/template>\n\u003C/UDashboardPanel>\n","vue",[140,283,284,332,338,344,350,356,362,368,374,380,386,392,398,404,410],{"__ignoreMap":161},[285,286,289,293,297,300,304,307,310,314,316,318,321,323,325,327,329],"span",{"class":287,"line":288},"line",1,[285,290,292],{"class":291},"sMK4o","\u003C",[285,294,296],{"class":295},"swJcz","UDashboardPanel",[285,298,299],{"class":291}," :",[285,301,303],{"class":302},"spNyl","id",[285,305,306],{"class":291},"=",[285,308,309],{"class":291},"\"",[285,311,313],{"class":312},"sTEyZ","panelId",[285,315,309],{"class":291},[285,317,299],{"class":291},[285,319,320],{"class":302},"resizable",[285,322,306],{"class":291},[285,324,309],{"class":291},[285,326,320],{"class":312},[285,328,309],{"class":291},[285,330,331],{"class":291},">\n",[285,333,335],{"class":287,"line":334},2,[285,336,337],{"class":312},"  \u003Ctemplate #header> Agent branding + status \u003C/template>\n",[285,339,341],{"class":287,"line":340},3,[285,342,343],{"class":312},"  \u003Ctemplate #body>\n",[285,345,347],{"class":287,"line":346},4,[285,348,349],{"class":312},"    \u003C!-- Messages loop -->\n",[285,351,353],{"class":287,"line":352},5,[285,354,355],{"class":312},"    \u003Cdiv v-for=\"msg in messages\">\n",[285,357,359],{"class":287,"line":358},6,[285,360,361],{"class":312},"      \u003C!-- User bubbles -->\n",[285,363,365],{"class":287,"line":364},7,[285,366,367],{"class":312},"      \u003C!-- Assistant: text + inline A2uiRenderer -->\n",[285,369,371],{"class":287,"line":370},8,[285,372,373],{"class":312},"      \u003CA2uiRenderer\n",[285,375,377],{"class":287,"line":376},9,[285,378,379],{"class":312},"        v-if=\"surfaces.get(surfaceId)\"\n",[285,381,383],{"class":287,"line":382},10,[285,384,385],{"class":312},"        :surface=\"surfaces.get(surfaceId)!\"\n",[285,387,389],{"class":287,"line":388},11,[285,390,391],{"class":312},"      />\n",[285,393,395],{"class":287,"line":394},12,[285,396,397],{"class":312},"    \u003C/div>\n",[285,399,401],{"class":287,"line":400},13,[285,402,403],{"class":312},"  \u003C/template>\n",[285,405,407],{"class":287,"line":406},14,[285,408,409],{"class":312},"  \u003Ctemplate #footer> Chat prompt \u003C/template>\n",[285,411,413,416,418],{"class":287,"line":412},15,[285,414,415],{"class":291},"\u003C/",[285,417,296],{"class":295},[285,419,331],{"class":291},[265,421,423],{"id":422},"useagentchatts",[140,424,425],{},"useAgentChat.ts",[131,427,428,429,432],{},"Composable at ",[140,430,431],{},"app/composables/useAgentChat.ts",". Currently uses canned responses with simulated streaming delays. In production, this becomes a WebSocket client.",[131,434,435,436,439,440,443],{},"Key design: the ",[140,437,438],{},"onA2uiLine"," callback feeds A2UI lines to the surface processor ",[135,441,442],{},"during streaming",", so surfaces render progressively — not only after the response completes.",[153,445,449],{"className":446,"code":447,"language":448,"meta":161,"style":161},"language-ts shiki shiki-themes material-theme-lighter material-theme material-theme-palenight","const { messages, status, send } = useAgentChat({\n  onA2uiLine: (line) => feed(line), // real-time surface updates\n});\n","ts",[140,450,451,489,521],{"__ignoreMap":161},[285,452,453,456,459,462,465,468,470,473,476,479,483,486],{"class":287,"line":288},[285,454,455],{"class":302},"const",[285,457,458],{"class":291}," {",[285,460,461],{"class":312}," messages",[285,463,464],{"class":291},",",[285,466,467],{"class":312}," status",[285,469,464],{"class":291},[285,471,472],{"class":312}," send ",[285,474,475],{"class":291},"}",[285,477,478],{"class":291}," =",[285,480,482],{"class":481},"s2Zo4"," useAgentChat",[285,484,485],{"class":312},"(",[285,487,488],{"class":291},"{\n",[285,490,491,494,497,500,503,506,509,512,515,517],{"class":287,"line":334},[285,492,493],{"class":481},"  onA2uiLine",[285,495,496],{"class":291},":",[285,498,499],{"class":291}," (",[285,501,287],{"class":502},"sHdIc",[285,504,505],{"class":291},")",[285,507,508],{"class":302}," =>",[285,510,511],{"class":481}," feed",[285,513,514],{"class":312},"(line)",[285,516,464],{"class":291},[285,518,520],{"class":519},"sHwdD"," // real-time surface updates\n",[285,522,523,525,527],{"class":287,"line":340},[285,524,475],{"class":291},[285,526,505],{"class":312},[285,528,529],{"class":291},";\n",[148,531,533],{"id":532},"production-evolution","Production Evolution",[131,535,536],{},"The mock → production path is clean:",[191,538,539,552],{},[194,540,541],{},[197,542,543,546,549],{},[200,544,545],{},"Layer",[200,547,548],{},"Mock (current)",[200,550,551],{},"Production",[207,553,554,570,583,596,609],{},[197,555,556,561,567],{},[212,557,558],{},[135,559,560],{},"Trans